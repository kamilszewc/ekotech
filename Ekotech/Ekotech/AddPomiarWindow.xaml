<Window x:Class="Ekotech.AddPomiarWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
        Title="Dodaj pomiar" Height="120" Width="240">
    <Grid>
        <Label Content="Czas wznoszenia: " HorizontalAlignment="Left" Margin="10,19,0,0" VerticalAlignment="Top"/>
        <xctk:DoubleUpDown x:Name="czasSpin" Value="0.0" Increment="0.1" Minimum="0.0" Maximum="1000.0" FormatString="F2" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="119,22,0,0"/>
        <Label Content="s" HorizontalAlignment="Left" Margin="199,19,0,0" VerticalAlignment="Top"/>
        <Button Content="Dodaj" HorizontalAlignment="Left" Margin="119,59,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>

    </Grid>
</Window>
